theory T_rvs_testing

use import rvs_matrix.Matrix
use import int.Int

function rvs_testing_A_Output1 int : matrix
function rvs_testing_B_Output1 int : matrix
function rvs_testing_rvsAdd_Output1 int : matrix
function rvs_testing_rvsAdd1_Output1 int : matrix
function rvs_testing_rvsAdd2_Output1 int : matrix
function rvs_testing_rvsAdd3_Output1 int : matrix
function rvs_testing_rvsSubtract_Output1 int : matrix

namespace NS_rvs_testing_A
  clone rvs_simulink.T_Constant with function out1 = rvs_testing_A_Output1
end

namespace NS_rvs_testing_B
  clone rvs_simulink.T_Constant with function out1 = rvs_testing_B_Output1
end

namespace NS_rvs_testing_rvsAdd
  clone rvs_simulink.T_rvsAdd with function in1 = rvs_testing_A_Output1, function in2 = rvs_testing_B_Output1, function out1 = rvs_testing_rvsAdd_Output1
end

namespace NS_rvs_testing_rvsAdd1
  clone rvs_simulink.T_rvsAdd with function in1 = rvs_testing_B_Output1, function in2 = rvs_testing_A_Output1, function out1 = rvs_testing_rvsAdd1_Output1
end

namespace NS_rvs_testing_rvsAdd2
  clone rvs_simulink.T_rvsAdd with function in1 = rvs_testing_rvsAdd1_Output1, function in2 = rvs_testing_rvsSubtract_Output1, function out1 = rvs_testing_rvsAdd2_Output1
end

namespace NS_rvs_testing_rvsAdd3
  clone rvs_simulink.T_rvsAdd with function in1 = rvs_testing_A_Output1, function in2 = rvs_testing_A_Output1, function out1 = rvs_testing_rvsAdd3_Output1
end

namespace NS_rvs_testing_rvsSubtract
  clone rvs_simulink.T_rvsSubtract with function in1 = rvs_testing_A_Output1, function in2 = rvs_testing_B_Output1, function out1 = rvs_testing_rvsSubtract_Output1
end

namespace NS_rvs_testing_rvsConstant
  goal G_rvs_testing_rvsConstant: forall k: int.  rvs_testing_rvsAdd2_Output1 k =  rvs_testing_rvsAdd2_Output1 (k-1)
end

namespace NS_rvs_testing_rvsEquiv
  goal G_rvs_testing_rvsEquiv: forall k: int.  rvs_testing_rvsAdd_Output1 k =  rvs_testing_rvsAdd1_Output1 k
end

namespace NS_rvs_testing_rvsEquiv1
  goal G_rvs_testing_rvsEquiv1: forall k: int.  rvs_testing_rvsAdd2_Output1 k =  rvs_testing_rvsAdd3_Output1 k
end

end
